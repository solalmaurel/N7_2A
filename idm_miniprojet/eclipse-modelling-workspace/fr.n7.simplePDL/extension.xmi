<?xml version="1.0" encoding="UTF-8"?>
<simplepdl:Process
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:simplepdl="http://simplepdl"
    xsi:schemaLocation="http://simplepdl SimplePDL.ecore"
    name="Optionnel">
  <activities
      linksToSuccessors="//@workSequences.0 //@workSequences.1"
      name="Conception"/>
  <activities
      linksToPredecessors="//@workSequences.1 //@workSequences.0"
      name="Documenter"/>
  <workSequences
      predecessor="//@activities.0"
      successor="//@activities.1"/>
  <workSequences
      linkType="finishToFinish"
      predecessor="//@activities.0"
      successor="//@activities.1"/>
</simplepdl:Process>
